{
  "name": "nestfile-cli",
  "version": "0.0.2",
  "description": "NestJs file generator CLI",
  "license": "MIT",
  "private": false,
  "author": "Viviane Florido @vivianeflowt",
  "repository": "viviflowt/makefile-cli.git",
  "homepage": "https://github.com/viviflowt/nestfile-cli#readme",
  "bugs": "https://github.com/viviflowt/nestfile-cli/issues",
  "keywords": [
    "applications",
    "create",
    "custom",
    "development",
    "file",
    "make"
  ],
  "bin": {
    "nestfile": "dist/main.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6.8.0",
    "yarn": ">=1.7.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check:dep": "npx depcheck",
    "check:format": "prettier --check **/*.ts",
    "check:npm": "npx npm-check -u",
    "commit": "cz",
    "dev": "ts-node-dev src/mocks/main.ts",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "release": "release-it",
    "test": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "test:ci": "jest --coverage --forceExit --runInBand --no-cache",
    "test:clear": "jest --clearCache",
    "test:cov": "jest --coverage --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.0",
    "conf": "^10.2.0",
    "eventemitter2": "^6.4.7",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@types/eslint": "^8.4.5",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^9.0.1",
    "@types/jest": "^28.1.7",
    "@types/lodash": "^4.14.183",
    "@types/node": "^18.7.6",
    "@types/prettier": "^2.7.0",
    "@types/weak-napi": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "auto-changelog": "^2.4.0",
    "commitizen": "^4.2.5",
    "cosmiconfig": "^7.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "expect-type": "^0.13.0",
    "jest": "^28.1.3",
    "jest-extended": "^3.0.2",
    "jest-ts-auto-mock": "^2.1.0",
    "prettier": "^2.7.1",
    "release-it": "^15.3.0",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "ttypescript": "^1.5.13",
    "type-fest": "^2.18.0",
    "typescript": "^4.7.4",
    "typesync": "^0.9.2",
    "weak-napi": "^2.0.2"
  }
}