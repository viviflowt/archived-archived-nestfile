{
  "name": "@nestmodules/common",
  "version": "0.0.1",
  "description": "Library for Rapid Development of NestJS applications",
  "author": "Viviane Florido @vivianeflowt",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write **/*.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "check:dep": "npx depcheck",
    "check:npm": "npx npm-check -u",
    "check:format": "prettier --check **/*.ts",
    "build:docs": "npx @compodoc/compodoc -p tsconfig.doc.json --name sdz-rule-engine --language=pt-BR --theme=Postmark --output=doc --disableCoverage",
    "test:ci": "jest --coverage --forceExit --runInBand --no-cache",
    "test:clear": "jest --clearCache"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@faker-js/faker": "^7.4.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expect-type": "^0.13.0",
    "jest": "^28.1.3",
    "jest-extended": "^3.0.2",
    "jest-ts-auto-mock": "^2.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "syncpack": "^8.2.4",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "tslib": "^2.4.0",
    "ttypescript": "^1.5.13",
    "type-fest": "^2.18.0",
    "typescript": "^4.7.4",
    "typesync": "^0.9.2",
    "weak-napi": "^2.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ]
  }
}